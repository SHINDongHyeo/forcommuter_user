{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "디버그",
        "program": "${workspaceFolder}/src/main.ts",
        "preLaunchTask": "npm: build",
        "outFiles": ["${workspaceFolder}/dist/**/*.js"],
        "envFile": "${workspaceFolder}/.env.development",
        "sourceMaps": true,
        "autoAttachChildProcesses": true,
        "restart": true,
        "console": "integratedTerminal"
      }
    ]
  }
  